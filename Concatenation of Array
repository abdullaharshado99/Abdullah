class Solution(object):
     def getConcatenation(self, nums):
        """
        :type nums: List[int]
        :rtype: List[int]
        """
        res = []
        for i in range(len(nums)):
            res.append(nums[i])
        nums.extend(res)
        return nums
     getConcatenation(None, [1,2,1])

// output = [1, 2, 1, 1, 2, 1]
