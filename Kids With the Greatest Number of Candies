def kidsWithCandies(self, candies, extraCandies):
        max_candies = max(candies)
        res = []
        for i in candies:
            if i + extraCandies >= max_candies:
                res.append(True)
            else:
                res.append(False)
        return res
kidsWithCandies(None, [2,3,5,1,3], 3)

// output = [true,true,true,false,true]
