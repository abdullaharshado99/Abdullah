def mostWordsFound(self, sentences):
        ress = []
        for i in range(len(sentences)):
            res = sentences[i]
            nod = res.split(' ')
            node = len(nod)
            ress.append(node)
        con = max(ress)
        return con
mostWordsFound(None, ["alice and bob love leetcode", "i think so too", "this is great thanks very much"])

// output = 6
