def shuffle(self, nums, n):
        x_ptr = 0
        y_ptr = 0
        res = []
        while x_ptr < n:
            res.append(nums[x_ptr])
            x_ptr += 1
            res.append(nums[y_ptr + n])
            y_ptr += 1
        return res 
shuffle(None, [2,5,1,3,4,7], 3)

// output = [2,3,5,4,1,7]
