def smallerNumbersThanCurrent(self, nums):
        result = []
        for i in range(len(nums)):
            count = 0
            for j in range(len(nums)):
                if j != i and nums[j] < nums[i]:
                    count += 1
            result.append(count)
        return result
smallerNumbersThanCurrent(None, [8,1,2,2,3])

// output = [4,0,1,1,3]
